{
  "name": "@umn-latis/cla-vue-template",
  "version": "3.0.0",
  "description": "Basic layout for CLA (LATIS) VueJS apps",
  "author": "LATIS Technology Architecture",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/UMN-LATIS/CLA-vue-template"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build && vue-tsc -b tsconfig.build.json",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "prettier": "prettier --write \"src/**/*.{js,ts,vue}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "npm run build && npx release-it"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.21",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.5.6",
    "postcss-nested-import": "^1.3.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vue": "^3.5.21",
    "vue-tsc": "^3.0.7"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
