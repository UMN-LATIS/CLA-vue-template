{
  "name": "@umn-latis/cla-vue-template",
  "version": "1.1.0-1",
  "description": "Basic layout for CLA (LATIS) VueJS apps",
  "author": "LATIS Technology Architecture",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/UMN-LATIS/CLA-vue-template"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build && vue-tsc -b tsconfig.build.json",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "prettier": "prettier --write \"src/**/*.{js,ts,vue}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "yarn build && release-it"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/blocks": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.4.1",
    "@storybook/vue3-vite": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.29",
    "postcss-nested-import": "^1.3.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^16.1.5",
    "storybook": "^7.4.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "publishConfig": {
    "access": "public"
  }
}
