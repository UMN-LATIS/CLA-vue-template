{
  "name": "@umn-latis/cla-vue-template",
  "version": "2.0.1",
  "description": "Basic layout for CLA (LATIS) VueJS apps",
  "author": "LATIS Technology Architecture",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/UMN-LATIS/CLA-vue-template"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build && vue-tsc -b tsconfig.build.json",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "prettier": "prettier --write \"src/**/*.{js,ts,vue}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "npm run build && npx release-it"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.29",
    "postcss-nested-import": "^1.3.0",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^2.0.29"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
